<view><view class="card"><view class="title border-bb">{{"可提现金额："+(cashinfo.balance_money||'0.00')+"元"}}</view><view class="x-flex title"><view class="title-icon">￥</view><input type="digit" placeholder="输入提现金额" data-event-opts="{{[['input',[['__set_model',['','getMoney','$event',[]]]]]]}}" value="{{getMoney}}" bindinput="__e"/></view></view><view class="card"><view class="type-title">提现方式</view><view class="type"><block wx:for="{{cashtype}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeType',['$0'],[[['cashtype','',index,'type']]]]]]]}}" class="{{['x-flex type-item',(current==item.type)?'type-active':'']}}" bindtap="__e"><view class="{{['mg-right',index==0?'cuIcon-card':index==1?'cuIcon-weixin':'cuIcon-pay']}}" style="{{(index==0?'color: #007AFF;':index==1?'color: #39B54A':'color: #007AFF')}}"></view><view>{{item.name}}</view></view></block></view></view><block wx:if="{{current==3}}"><view class="card"><view class="list x-flex border-bb"><view class="list-label">持卡人姓名</view><input type="text" placeholder="请输入卡号对应的姓名" disabled="true" data-event-opts="{{[['input',[['__set_model',['','cardholder','$event',[]]]]]]}}" value="{{cardholder}}" bindinput="__e"/></view><view class="list x-flex"><view class="list-label">卡号</view><input type="text" placeholder="请输入正确的银行卡账号" disabled="true" data-event-opts="{{[['input',[['__set_model',['','card_number','$event',[]]]]]]}}" value="{{card_number}}" bindinput="__e"/></view><view class="list x-flex"><view class="list-label">开户行</view><input type="text" placeholder="请输入正确的银行开户行" disabled="true" data-event-opts="{{[['input',[['__set_model',['','bank_name','$event',[]]]]]]}}" value="{{bank_name}}" bindinput="__e"/></view></view></block><block wx:if="{{current==1}}"><view class="card"><view class="list x-flex border-bb"><view class="list-label">收款人</view><input type="text" placeholder="请输入提现微信名称" disabled="true" data-event-opts="{{[['input',[['__set_model',['','wechat_name','$event',[]]]]]]}}" value="{{wechat_name}}" bindinput="__e"/></view><view class="list x-flex border-bb"><view class="list-label">微信号</view><input type="text" placeholder="请输入提现微信号" disabled="true" data-event-opts="{{[['input',[['__set_model',['','wechat_number','$event',[]]]]]]}}" value="{{wechat_number}}" bindinput="__e"/></view></view></block><block wx:if="{{current==2}}"><view class="card"><view class="list x-flex border-bb"><view class="list-label">收款人</view><input type="text" placeholder="请输入提现支付宝名称" disabled="true" data-event-opts="{{[['input',[['__set_model',['','alipay_name','$event',[]]]]]]}}" value="{{alipay_name}}" bindinput="__e"/></view><view class="list x-flex border-bb"><view class="list-label">支付宝号</view><input type="text" placeholder="请输入提现支付宝号" disabled="true" data-event-opts="{{[['input',[['__set_model',['','alipay_account','$event',[]]]]]]}}" value="{{alipay_account}}" bindinput="__e"/></view></view></block><form report-submit="true" data-event-opts="{{[['submit',[['sure',['$event']]]]]}}" bindsubmit="__e"><button class="my-btn" formType="submit" type="primary">提交申请</button></form></view>