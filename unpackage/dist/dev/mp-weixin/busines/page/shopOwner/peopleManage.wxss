.editIcon {
  width: 30rpx;
  height: 30rpx;
}
.content {
  padding-left: 30rpx;
}
.top {
  width: 100%;
  position: fixed;
  top: 0;
  z-index: 2;
}
.box {
  height: 100rpx;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.pad-10 {
  padding: 10rpx 0;
}
.left {
  width: 170rpx;
  line-height: 84rpx;
  text-align: center;
  font-size: 28rpx;
  color: #555;
  background: #f8f8f8;
}
.right {
  background: #fff;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  padding: 0 0 0 35rpx;
}
.active {
  background: #fff;
  color: #FEB026;
  font-size: 28rpx;
}
.username {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 100rpx;
}
.type {
  width: 15%;
  text-align: left;
}
.scrollBox {
  max-height: 500rpx;
}
.cancel {
  padding: 30rpx 0;
  font-size: 30rpx;
  border-top: 1rpx solid #EEEEEE;
  box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.1);
}
.list {
  padding: 30rpx 30rpx 30rpx 0;
  background-color: #FFFFFF;
}
.bor-b {
  border-bottom: 1rpx solid #EEEEEE;
}
.bor-t {
  border-top: 1rpx solid #EEEEEE;
}
.mar-10 {
  margin: 0 10rpx;
}

