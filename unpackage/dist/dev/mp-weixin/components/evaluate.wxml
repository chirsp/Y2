<view><block wx:for="{{listeva}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="evapaper"><image class="evaimg" src="{{item.avatarurl||'../static/head.png'}}"></image><view class="details"><view class="name  flex_row flex_ai"><text>{{item.nickname||'匿名用户'}}</text><block wx:for="{{5}}" wx:for-item="items" wx:for-index="indexs" wx:key="indexs"><view class="ViewStar"><image class="StarImg" src="{{item.score>=indexs?'/static/star.png':'/static/blackstar.png'}}"></image></view></block></view><view class="Star"><text class="data">{{item.ctime}}</text></view><view class="matter"><view class="mattertxt">{{item.content}}</view><block wx:for="{{item.images}}" wx:for-item="ite" wx:for-index="ind" wx:key="ind"><block wx:if="{{item.images.length}}"><image class="matterImg" src="{{ite}}" mode="aspectFill" data-event-opts="{{[['tap',[['lookImg',['$0',ind],[[['listeva','',index,'images']]]]]]]}}" bindtap="__e"></image></block></block></view><view class="answer" style="{{(item.seller_content?'display:block':'display:none')}}">商家回复:<text class="answertxt">{{item.seller_content}}</text></view></view></view></block><view class="space"></view><block wx:if="{{listeva.length==0}}"><nodata vue-id="4a625f32-1" msg="评价空空如也~" bind:__l="__l"></nodata></block></view>