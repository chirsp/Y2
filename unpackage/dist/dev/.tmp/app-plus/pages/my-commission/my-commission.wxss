.noData {
  line-height: 80rpx;
  text-align: center;
  color: #888;
}
.x-sticky {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 980;
}
.top {
  height: 290rpx;
  padding-top: 20rpx;
  background-color: #FC6840;
}
.add-cash {
  font-size: 26rpx;
  padding: 2rpx 14rpx;
  color: #fff;
  border: 1rpx solid #fff;
  border-radius: 10rpx;
}
.top-img image {
  width: 85rpx;
  height: 85rpx;
  border-radius: 50%;
}
.top-1 {
  color: #FFFFFF;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
