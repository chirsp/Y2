<view><view class="page"><image class="bk-img" src="{{data.banner_ids?data.banner_ids:'/static/bg.jpg'}}" mode="aspectFill"></image><view class="bk-content"><view class="welcome-block"><view class="welcome-headimg"><image class="welcome-headimg-ico" src="{{data.logo?data.logo:'/static/head.png'}}"></image></view><view class="welcome-title">{{'欢迎来到'+(data.name?data.name:'本店')+''}}</view></view><view class="order-people"><view class="order-title1">客官,您几位?</view><view class="order-title2">{{"桌号:"+deskId}}<block wx:if="{{config.canwei_price}}"><text style="padding-left:30rpx;">{{"餐位费: "+config.canwei_price+"元/人"}}</text></block></view><view class="people-num-all"><block wx:for="{{more}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['setNums',[index+1]]]]]}}" class="{{['people-input',(personNums==index+1)?'checked':'']}}" bindtap="__e">{{index+1+" 人"}}</view></block></block><view data-event-opts="{{[['tap',[['showNum',['$event']]]]]}}" class="people-input" bindtap="__e">更多</view></view><view data-event-opts="{{[['tap',[['goStore']]]]}}" class="saoma-btn-block" bindtap="__e"><view>确定</view></view></view></view></view><view class="{{['cu-modal',num_show?'show':'']}}"><view class="cu-dialog" style="width:70%;"><view class="cu-bar bg-white justify-end"><view class="content">就餐人数</view><view data-event-opts="{{[['tap',[['hideNum',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="padding-xl" style="padding:40rpx 60rpx;background:#fff;"><input class="person-input" type="number" value="{{personNums}}" data-event-opts="{{[['input',[['__set_model',['','personNums','$event',[]]]]]]}}" bindinput="__e"/></view><view class="cu-bar bg-white"><view data-event-opts="{{[['tap',[['hideNum',['$event']]]]]}}" class="action margin-0 flex-sub  solid-left" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['goStore',['$event']]]]]}}" class="action margin-0 flex-sub text-green solid-left" bindtap="__e">确定</view></view></view></view></view>