<view><view class="top"><view class="mBg topbanner"><view class="flex_row topSelect"><block wx:for="{{dimension}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['selectType',[index]]]]]}}" class="{{['item',dIndex==index?'mB33 _cff':index?'bor-left':'']}}" bindtap="__e">{{item}}</text></block></view></view><block wx:if="{{dIndex}}"><scroll-view class="bg-white nav" scroll-x scroll-with-animation scroll-left="{{scrollLeft}}"><view class="flex text-center"><block wx:for="{{Time}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['timeSelect',[index,'$0','$1'],[[['Time','',index,'year']],[['Time','',index,'month']]]]]]]}}" class="{{['cu-item flex-sub',index==TabCur?'text-orange cur _f32':'noselect']}}" bindtap="__e">{{''+item.name+''}}</view></block></view></scroll-view></block></view><view style="{{(dIndex?'height:100px;':'height:50px')}}"></view><view class="flex_col total-num _c99 _f30"><text>{{"营业额(元):￥"+(toatal.money||'0.00')}}</text></view><view class="qiun-charts"><canvas class="charts" canvas-id="canvasLineA" id="canvasLineA" data-event-opts="{{[['touchstart',[['touchLineA',['$event']]]],['touchmove',[['moveLineA',['$event']]]],['touchend',[['touchEndLineA',['$event']]]]]}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"></canvas></view><view class="dishTotal flex_col"><text class="title _c99 _f30">菜品销量</text><block wx:if="{{food_count.length==0}}"><nodata vue-id="1" msg="暂无菜品销量统计数据~" bind:__l="__l"></nodata></block><block wx:for="{{food_count}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{food_count.length}}"><view class="title flex_col"><view class="flex_sb flex_row"><text class="_fw _c33">{{item.name}}</text><text class="_c66 _fw">{{"￥"+item.money+"/"+item.num+"份"}}</text></view><progress loading="{{loading}}" num="{{item.percent_money}}"></progress></view></block></block></view></view>